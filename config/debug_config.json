{
  "solver": {
    "mode": "debug_single"
  },
  "acceleration": {
    "force_gpu_off": true,
    "enable_cuda_nltk": false
  },
  "dictionaries": {
    "force_single_dictionary": "/usr/share/dict/american-english"
  },
  "filtering": {
    "min_word_length": 4,
    "enable_nyt_rejection_filter": true
  },
  "output": {
    "minimal_stats": true,
    "verbose_stats": false
  },
  "logging": {
    "level": "INFO"
  }
}